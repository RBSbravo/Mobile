{
  "expo": {
    "name": "MITO Task Manager",
    "slug": "mito-task-manager",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/mito_logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/mito_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#667eea"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bravorb.mitomobileapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/mito_logo.png",
        "backgroundColor": "#667eea"
      },
      "package": "com.bravorb.mitomobileapp"
    },
    "web": {
      "favicon": "./assets/mito_logo.png",
      "bundler": "metro"
    },
    "scheme": "mito",
    "extra": {
      "eas": {
        "projectId": "20d4180e-0dae-4b52-ab09-4a8b949938ff"
      }
    }
  }
}
